# Neptune Database Configuration
# Copy this file to .env and fill in your AWS Neptune details

# Neptune cluster endpoint (without protocol or port)
# Example: my-neptune-cluster.cluster-xxxxx.us-east-1.neptune.amazonaws.com
NEPTUNE_ENDPOINT=

# S3 bucket for Neptune bulk loading
# Example: my-neptune-bulk-load-bucket
NEPTUNE_S3_BUCKET=

# IAM role ARN with Neptune bulk load permissions
# Example: arn:aws:iam::123456789012:role/NeptuneLoadFromS3
NEPTUNE_IAM_ROLE=

# AWS Region
# Example: us-east-1
AWS_REGION=us-east-1

# AWS Credentials (if not using AWS CLI credentials)
# Leave blank to use AWS CLI configured credentials or IAM role
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
